{
  "JyMediaAnimation": {
    "display_name": "剪映带动画的媒体",
    "inputs": {
      "file_path": {
        "name": "文件路径",
        "tooltip": "图片或视频文件的完整路径"
      },
      "start_in_media": {
        "name": "媒体内开始时间",
        "tooltip": "在原始媒体中的开始时间（秒）"
      },
      "start_at_track": {
        "name": "轨道开始时间",
        "tooltip": "在草稿轨道中的开始时间（秒），为0时自动拼接在上一个素材末尾"
      },
      "duration": {
        "name": "持续时间",
        "tooltip": "媒体在草稿中的持续时间（秒）"
      },
      "meida_group": {
        "name": "媒体组",
        "tooltip": "输入的媒体组，用于连续添加多个媒体"
      },
      "animation_in": {
        "name": "入场动画",
        "tooltip": "媒体的入场动画效果"
      },
      "animation_group": {
        "name": "动画组",
        "tooltip": "媒体的中途动画效果"
      },
      "animation_out": {
        "name": "出场动画",
        "tooltip": "媒体的出场动画效果"
      }
    },
    "outputs": {
      "0": {
        "name": "带动画的媒体"
      },
      "1": {
        "name": "媒体组"
      }
    }
  },
