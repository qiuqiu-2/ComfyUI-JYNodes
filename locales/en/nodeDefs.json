{
  "JyMediaAnimation": {
    "display_name": "JianYing Animated Media",
    "inputs": {
      "file_path": {
        "name": "Media Path",
        "tooltip": "Full path to image or video file"
      },
      "start_in_media": {
        "name": "Start Time",
        "tooltip": "Start time in original media (seconds)"
      },
      "start_at_track": {
        "name": "Track Start Time",
        "tooltip": "Start time in draft track (seconds), 0 means automatically append after previous material"
      },
      "duration": {
        "name": "Duration",
        "tooltip": "Duration of media in draft (seconds)"
      },
      "meida_group": {
        "name": "Media Group",
        "tooltip": "Input media group for adding multiple media continuously"
      },
      "animation_in": {
        "name": "In Animation",
        "tooltip": "Media entrance animation effect"
      },
      "animation_group": {
        "name": "Animation Group",
        "tooltip": "Media middle animation effect"
      },
      "animation_out": {
        "name": "Out Animation",
        "tooltip": "Media exit animation effect"
      }
    },
    "outputs": {
      "0": {
        "name": "Animated Media"
      },
      "1": {
        "name": "Media Group"
      }
    }
  },
